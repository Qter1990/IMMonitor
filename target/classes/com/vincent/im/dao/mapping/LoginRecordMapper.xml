<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vincent.im.dao.LoginRecordDao" >
	<insert id="insert" parameterType="com.vincent.im.model.LoginRecord">
		INSERT INTO login_record  (accid,login_operation,event_type,client_ip,client_type,code,sdk_version,timestamp,create_time,update_time,delete_time)
		VALUES (#{accid},#{login_operation},#{event_type},#{client_ip},#{client_type},#{code},#{sdk_version},#{timestamp},#{create_time},#{update_time},#{delete_time})
	</insert>
</mapper>